#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/vuePressInKube/docs/node_modules/.pnpm/html-minifier@3.5.21/node_modules/html-minifier/node_modules:/home/user/vuePressInKube/docs/node_modules/.pnpm/html-minifier@3.5.21/node_modules:/home/user/vuePressInKube/docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/vuePressInKube/docs/node_modules/.pnpm/html-minifier@3.5.21/node_modules/html-minifier/node_modules:/home/user/vuePressInKube/docs/node_modules/.pnpm/html-minifier@3.5.21/node_modules:/home/user/vuePressInKube/docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
